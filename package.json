{
  "name": "status.kunruchcreations.com",
  "version": "0.2.3",
  "description": "Displays monitoring information of all sites of KunRuch Creations via UptimeRobot",
  "dependencies": {
    "chart.js": "^2.3.0",
    "mmcss": "^0.2.4",
    "vue": "^2.0.0-rc.1",
    "vue-resource": "^1.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "babelify": "^7.3.0",
    "cross-env": "^2.0.1",
    "mmpilot": "^0.2.1",
    "node-sass": "^3.10.0",
    "npm-run-all": "^3.1.0",
    "pug": "^0.1.0",
    "vueify": "^9.2.4"
  },
  "scripts": {
    "clean": "mmpilot clean",
    "build": "cross-env NODE_ENV=production mmpilot build",
    "dev-build": "mmpilot build -c _mmpilot.yml,_mmpilot-dev.yml",
    "watch": "mmpilot watch -c _mmpilot.yml,_mmpilot-dev.yml",
    "serve": "mmpilot serve",
    "dev": "npm-run-all clean dev-build --parallel watch serve",
    "deploy": "npm-run-all clean build deploy"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "engines": {
    "node": ">=4.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunruch/status.kunruchcreations.com.git"
  },
  "author": "Kanishk Kunal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kunruch/status.kunruchcreations.com/issues"
  },
  "homepage": "https://status.kunruchcreations.com/"
}
